def pesquisa_binaria (lista, item):

baixo 00

alto len(lista) - 10

while baixo <= alto:

meio = (baixo alto) / 20

chute lista[meio]

if chute = item:

return meio

if chute > item:

alto meio 1

else:

baixo meio + 1

return None

minha_lista [1, 3, 5, 7, 9]

print pesquisa_binaria (minha_lista, 3) # => 10

print pesquisa_binaria (minha_lista, 1) # => None
